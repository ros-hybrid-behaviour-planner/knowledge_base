<!-- Runs all tests, contained by knowledge_base package -->
<launch>
    <node pkg="knowledge_base" type="knowledge_base_node.py" name ="kb" />
    <test pkg="knowledge_base" type="knowledge_base_manager_test.py" test-name="knowledge_base_manager_test_node"/>
    <test pkg="knowledge_base" type="update_handler_test.py" test-name="update_handler_test_node"/>
    <include file="$(find knowledge_base)/launch/unit_tests.test" />
</launch>